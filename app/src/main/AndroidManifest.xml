            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.jake.helloworld1.MainActivity" />
        </activity>
        <activity
            android:name=".Gordons_cafe"
            android:label="@string/title_activity_gordons_cafe"
            android:parentActivityName=".MainActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.jake.helloworld1.MainActivity" />
        </activity>
        <activity
            android:name=".George_farah_cafe"
            android:label="@string/title_activity_george_farah_cafe"
            android:parentActivityName=".MainActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.jake.helloworld1.MainActivity" />
        </activity>
    </application>

</manifest>
